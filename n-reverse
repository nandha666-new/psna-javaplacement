import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        long n,pd,r=0;
        n = in.nextLong();
        while(n>0){
            pd = n%10;
            n =n/10;
            r=r*10+pd;
        }
        System.out.println(r);
    }
}
