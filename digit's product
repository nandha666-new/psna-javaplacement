import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in =new Scanner(System.in);
        int n,ld,pro=1;
        n=in.nextInt();
        while(n>0){
            ld = n%10;
            pro = pro * ld;
            n = n/10;
            
        }
        System.out.println(pro);
    }
}
